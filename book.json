{
  "language": "zh-hans",
  "author": "Zander Hsueh",
  "sharing": false,
  "plugins": [
    "code",
    "anchor-navigation-ex",
    "page-footer-ex",
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "splitter",
    "hide-element",
    "insert-logo",
    "expandable-chapters-small",
    "popup"
  ],
  "pluginsConfig": {
    "page-footer-ex": {
      "copyright": "[Zander Hsueh](https://xuezenghui.com)",
      "markdown": true,
      "update_label": "<i>updated</i>",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "anchor-navigation-ex": {
      "multipleH1": false
    },
    "insert-logo": {
      "url": "/images/logo.png",
      "style": "background: none; max-height: 60px; min-height: 60px"
    }
  }
}
